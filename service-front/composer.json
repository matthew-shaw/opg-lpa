{
    "name": "ministryofjustice/opg-lpa-front",
    "description": "Front end service for creating Lasting Power of Attorney PDF documents",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "framework",
        "zf2"
    ],
    "homepage": "http://framework.zend.com/",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ministryofjustice/opg-lpa-datamodels",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/ministryofjustice/opg-lpa-logger",
            "no-api": true
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^6.3",
        "aws/aws-sdk-php": "^3.0",
        "ministryofjustice/opg-lpa-datamodels": "^13.5",
        "ministryofjustice/opg-lpa-logger": "^5.0",
        "michelf/php-markdown": "^1.8",
        "alphagov/pay-integration-php": "^1.0",
        "php-http/guzzle6-adapter": "^1.1",
        "soundasleep/html2text": "^0.5.0",
        "laminas/laminas-component-installer": "^1.1",
        "laminas/laminas-mvc": "^3.1",
        "laminas/laminas-authentication": "^2.5",
        "laminas/laminas-cache": "^2.7",
        "laminas/laminas-form": "^2.11",
        "laminas/laminas-session": "^2.8",
        "laminas/laminas-math": "^3.0",
        "laminas/laminas-crypt": "^3.2",
        "kokspflanze/zfc-twig": "^2.1",
        "laminas/laminas-mvc-i18n": "^1.0",
        "laminas/laminas-mail": "^2.8",
        "laminas/laminas-json": "^3.1",
        "laminas/laminas-console": "^2.7",
        "laminas/laminas-test": "^3.1",
        "sendgrid/sendgrid": "^6.2",
        "laminas/laminas-mvc-console": "^1.2",
        "theiconic/php-ga-measurement-protocol": "^2.0",
        "guzzlehttp/psr7": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/"
        }
    }
}
